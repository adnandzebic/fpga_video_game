 455                                  ;; Draw Block - Draw Block Subroutine
 455                                  ;;============================================================================
 455                      draw_block: 
 455                                  ;;============================================================================
 455                                  ;; Draws Top Line of Block
 455                                  ;;============================================================================
 455  005E0                           LOAD s5, sE                           ; load 255 into s5 (horizontal center of block)
 456  016E1                           LOAD s6, E1                           ; load 245 into s6
 457  1C650                           COMPARE s6, s5                        ; checking if the block is done "peaking out" from the right (initial compare: 245 ? 255)
 458  3A45E                           JUMP C, 45E[right_edge]               ; jump to right_edge
 459  0160A                           LOAD s6, 0A                           ; load 10 into s6
 45A  1C560                           COMPARE s5, s6                        ; comparing s5 and s6
 45B  3A462                           JUMP C, 462[left_edge]                ; jump to left_edge
 45C  32462                           JUMP Z, 462[left_edge]                ; jump to left_edge
 45D  22466                           JUMP 466[in_middle]                   ; jump to in_middle
 45E  2D500               right_edge: OUTPUT s5, 00[stax]                   ; set desired stax
 45F  1950A                           SUB s5, 0A                            ;
 460  2D502                           OUTPUT s5, 02[endx]                   ; set desired stax
 461  22469                           JUMP 469[finished1]                   ; jump to draw_ys
 462  2D500                left_edge: OUTPUT s5, 00[stax]                   ; set desired stax
 463  01600                           LOAD s6, 00                           ; put 255 into s5
 464  2D602                           OUTPUT s6, 02[endx]                   ; set desired stax
 465  22469                           JUMP 469[finished1]                   ; jump to draw_ys
 466                       in_middle: 
 466  2D500                           OUTPUT s5, 00[stax]                   ; set desired stax
 467  1950A                           SUB s5, 0A                            ; subtract the width of block to get the other x coordinate
 468  2D502                           OUTPUT s5, 02[endx]                   ; set desired endx
 469  2DF01                finished1: OUTPUT sF, 01[stay]                   ; set desired stay
 46A  2DF03                           OUTPUT sF, 03[endy]                   ; set desired endy
 46B  20528                           CALL 528[old_draw_line]               ; draws a line (careful: this subroutine overwrites s5)
 46C                                  ;;============================================================================
