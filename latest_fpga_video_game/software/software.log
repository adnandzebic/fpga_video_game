509                             ;;============================================================================

509        random_block_location:
509  1DE00                      COMPARE se, 00                 ; is it time to create a new variable?
50A  3250F                      JUMP Z, new_random             ; jump to new random creator
50B  1DEFF                      COMPARE se, FF                 ; is it time to create a new variable?
50C  3250F                      JUMP Z, new_random             ; jump to new random creator
50D  3650E                      JUMP NZ, skip_everything       ; goes back

50E  25000     skip_everything: RETURN                         ; returns back (no new rand. number)

50F  09507          new_random: INPUT s5, prng                 ; create to new random number
510  1D50F                      COMPARE s5, 0F                 ; check if it's in range
511  3E513                      JUMP NC, check_other_bound_range ; check lower bound of map
512  3A50F                      JUMP C, new_random             ; get new variable

513        check_other_bound_range:
513  1D5B4                      COMPARE s5, B4                 ; compare s5 to 180
514  3E50F                      JUMP NC, new_random            ; get a new random number
515  00F50                      LOAD sf, s5                    ; a new y coordinate generated
516  25000                      RETURN                         ; get out of subroutine

