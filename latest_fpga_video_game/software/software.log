 460                      draw_block: 
 460                                  ;;============================================================================
 460                                  ;; Draws Top Line of Block
 460                                  ;;============================================================================
 460  005E0                           LOAD s5, sE                           ; load 255 into s5 (horizontal center of block)
 461  016E1                           LOAD s6, E1                           ; load 245 into s6
 462  1C650                           COMPARE s6, s5                        ; checking if the block is done "peaking out" from the right (initial compare: 245 ? 255)
 463  3A469                           JUMP C, 469[right_edge]               ; jump to right_edge
 464  0160A                           LOAD s6, 0A                           ; load 10 into s6
 465  1C560                           COMPARE s5, s6                        ; comparing s5 and s6
 466  3A46D                           JUMP C, 46D[left_edge]                ; jump to left_edge
 467  3246D                           JUMP Z, 46D[left_edge]                ; jump to left_edge
 468  22471                           JUMP 471[in_middle]                   ; jump to in_middle
 469  2D500               right_edge: OUTPUT s5, 00[stax]                   ; set desired stax
 46A  1950A                           SUB s5, 0A                            ;
 46B  2D502                           OUTPUT s5, 02[endx]                   ; set desired stax
 46C  22474                           JUMP 474[finished1]                   ; jump to draw_ys
 46D  2D500                left_edge: OUTPUT s5, 00[stax]                   ; set desired stax
 46E  01600                           LOAD s6, 00                           ; put 255 into s5
 46F  2D602                           OUTPUT s6, 02[endx]                   ; set desired stax
 470  22474                           JUMP 474[finished1]                   ; jump to draw_ys
 471                       in_middle: 
 471  2D500                           OUTPUT s5, 00[stax]                   ; set desired stax
 472  1950A                           SUB s5, 0A                            ; subtract the width of block to get the other x coordinate
