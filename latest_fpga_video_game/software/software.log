 46C                                  ;;============================================================================
 46C  005F0                           LOAD s5, sF                           ; load sF into s5
 46D  1153C                           ADD s5, 3C                            ; add 40 to s5
 46E  2D501                           OUTPUT s5, 01[stay]                   ; set desired stay
 46F  2D503                           OUTPUT s5, 03[endy]                   ; set desired endy
 470  20528                           CALL 528[old_draw_line]               ; draws a line (careful: this subroutine overwrites s5)
