 88C                                  ;; Draws Top Line of Block
 88C                                  ;;============================================================================
 88C  005E0                           LOAD s5, sE                           ; load 255 into s5 (horizontal center of block)
 88D  016E1                           LOAD s6, E1                           ; load 245 into s6
 88E  1C650                           COMPARE s6, s5                        ; checking if the block is done "peaking out" from the right (initial compare: 245 ? 255)
 88F  3A895                           JUMP C, 895[right_edge]               ; jump to right_edge
 890  0160A                           LOAD s6, 0A                           ; load 10 into s6
 891  1C560                           COMPARE s5, s6                        ; comparing s5 and s6
 892  3A899                           JUMP C, 899[left_edge]                ; jump to left_edge
 893  32899                           JUMP Z, 899[left_edge]                ; jump to left_edge
 894  2289D                           JUMP 89D[in_middle]                   ; jump to in_middle
 895  2D500               right_edge: OUTPUT s5, 00[stax]                   ; set desired stax
 896  1950A                           SUB s5, 0A                            ;
 897  2D502                           OUTPUT s5, 02[endx]                   ; set desired stax
