4DC                             ;; Draw Block - Draw Block Subroutine
4DC                             ;;============================================================================

4DC                 draw_block:

4DC                             ;;============================================================================
4DC                             ;; Draws Top Line of Block
4DC                             ;;============================================================================

4DC  005E0                      LOAD s5, se                    ; load 255 into s5 (horizontal center of block)
4DD  016E1                      LOAD s6, E1                    ; load 245 into s6
4DE  1C650                      COMPARE s6, s5                 ; checking if the block is done "peaking out" from the right (initial compare: 245 ? 255)
4DF  3A4E5                      JUMP C, right_edge             ; jump to right_edge
4E0  0160A                      LOAD s6, 0A                    ; load 10 into s6
4E1  1C560                      COMPARE s5, s6                 ; comparing s5 and s6
4E2  3A4E9                      JUMP C, left_edge              ; jump to left_edge
4E3  324E9                      JUMP Z, left_edge              ; jump to left_edge
4E4  224ED                      JUMP in_middle                 ; jump to in_middle

4E5  2D500          right_edge: OUTPUT s5, stax                ; set desired stax
4E6  1950A                      SUB s5, 0A                     ;
4E7  2D502                      OUTPUT s5, endx                ; set desired stax
4E8  224F0                      JUMP finished1                 ; jump to draw_ys

4E9  2D500           left_edge: OUTPUT s5, stax                ; set desired stax
4EA  01600                      LOAD s6, 00                    ; put 255 into s5
4EB  2D602                      OUTPUT s6, endx                ; set desired stax
4EC  224F0                      JUMP finished1                 ; jump to draw_ys

4ED                  in_middle:
4ED  2D500                      OUTPUT s5, stax                ; set desired stax
4EE  1950A                      SUB s5, 0A                     ; subtract the width of block to get the other x coordinate
4EF  2D502                      OUTPUT s5, endx                ; set desired endx

4F0  2DF01           finished1: OUTPUT sf, stay                ; set desired stay
4F1  2DF03                      OUTPUT sf, endy                ; set desired endy
4F2  205AF                      CALL old_draw_line             ; draws a line (careful: this subroutine overwrites s5)

4F3                             ;;============================================================================
4F3                             ;; Draws Bottom Line of Block
4F3                             ;;============================================================================

4F3  005F0                      LOAD s5, sf                    ; load sF into s5
4F4  1153C                      ADD s5, 3C                     ; add 40 to s5
4F5  2D501                      OUTPUT s5, stay                ; set desired stay
4F6  2D503                      OUTPUT s5, endy                ; set desired endy
4F7  205AF                      CALL old_draw_line             ; draws a line (careful: this subroutine overwrites s5)

4F8                             ;;============================================================================
4F8                             ;; Draws Right Vertical Line of Block
4F8                             ;;============================================================================

4F8  2DE00                      OUTPUT se, stax                ; set desired stax
4F9  2DE02                      OUTPUT se, endx                ; set desired endx
4FA  2DF01                      OUTPUT sf, stay                ; set desired stay
4FB  005F0                      LOAD s5, sf                    ; load sF into s5
4FC  1153C                      ADD s5, 3C                     ; add 60 to y coordinate
4FD  2D503                      OUTPUT s5, endy                ; set desired endy
4FE  205AF                      CALL old_draw_line             ; draws a line (careful: this subroutine overwrites s5)

4FF                             ;;============================================================================
4FF                             ;; Draws Left Vertical Line of Block
4FF                             ;;============================================================================

4FF  005E0                      LOAD s5, se                    ; load sE into s5
500  1950A                      SUB s5, 0A                     ; subtract 10 from s5
501  2D500                      OUTPUT s5, stax                ; set desired stax
502  2D502                      OUTPUT s5, endx                ; set desired endx
503  2DF01                      OUTPUT sf, stay                ; set desired stay
504  005F0                      LOAD s5, sf                    ; load sF into s5
505  1153C                      ADD s5, 3C                     ; add 60 to y coordinate
506  2D503                      OUTPUT s5, endy                ; set desired endy
507  205AF                      CALL old_draw_line             ; draws a line (careful: this subroutine overwrites s5)
508  25000                      RETURN                         ; finished with draw_block subroutine



